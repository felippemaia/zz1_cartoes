<mvc:View xmlns:calendar="sap.ui.unified.calendar" xmlns:cards="sap.f.cards" controllerName="zz1cartoes.controller.View3" height="100%"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:core="sap.ui.core"
    xmlns:layout="sap.ui.layout"
    xmlns="sap.m"
    xmlns:f="sap.f"
    xmlns:fb="sap.ui.comp.filterbar"
    xmlns:vm="sap.ui.comp.variants"
    xmlns:form="sap.ui.layout.form">
    <App id="app">
        <pages>
            <Page title="{i18n>edittitle}" id="page" showHeader="true" showNavButton="true" navButtonPress="_onPageNavButtonPress">
                
                <content>
                    <form:Form id="_IDGenForm1" width="100%" editable="true" visible="true">
                        <form:title/>
                        <form:formContainers>
                            <form:FormContainer id="_IDGenFormContainer1" visible="true">
                                <form:formElements>
                                    <form:FormElement id="_IDGenFormElement1" visible="true">
                                        <form:label>
                                            <Label id="_IDGenLabel1" text="{i18n>lblMATNR}" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
                                        </form:label>
                                        <form:fields>
                                            <Text text="" id="tMatnr" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
                                        </form:fields>
                                    </form:FormElement>
                                    <form:FormElement id="_IDGenFormElement2" visible="true">
                                        <form:label>
                                            <Label id="_IDGenLabel2" text="{i18n>lblAUFNR}" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
                                        </form:label>
                                        <form:fields>
                                            <Text text="" id="tAUFNR" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
                                        </form:fields>
                                    </form:FormElement>
                                    <form:FormElement id="_IDGenFormElement3" visible="true">
                                        <form:label>
                                            <Label id="_IDGenLabel3" text="{i18n>lblReserva}" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
                                        </form:label>
                                        <form:fields>
                                            <Text text="" width="auto" id="tReserva" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
                                        </form:fields>
                                    </form:FormElement>
                                    <form:FormElement id="_IDGenFormElement31" visible="true">
                                        <form:label>
                                            <Label id="_IDGenLabel31" text="{i18n>lblItemReserva}" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
                                        </form:label>
                                        <form:fields>
                                            <Text text="" width="auto" id="tItemReserva" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
                                        </form:fields>
                                    </form:FormElement>
                                    <form:FormElement id="_IDGenFormElement4" visible="true">
                                        <form:label>
                                            <Label id="_IDGenLabel4" text="{i18n>lblTpFracionamento}" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
                                        </form:label>
                                        <form:fields>
                                            <Text text="" width="auto" id="tTpFran" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
                                        </form:fields>
                                    </form:FormElement>
                                </form:formElements>
                                <form:title/>
                            </form:FormContainer>
                            <form:FormContainer id="_IDGenFormContainer2" visible="true">
                                <form:formElements>
                                    <form:FormElement id="_IDGenFormElement5" visible="true">
                                        <form:label>
                                            <Label id="_IDGenLabel5" text="{i18n>lblLGORT}" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
                                        </form:label>
                                        <form:fields>
                                            <Text text="" width="auto" id="tLgt" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
                                        </form:fields>
                                    </form:FormElement>
                                    <form:FormElement id="_IDGenFormElement6" visible="true">
                                        <form:label>
                                            <Label id="_IDGenLabel6" text="{i18n>lblQtdNesc}" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
                                        </form:label>
                                        <form:fields>
                                            <Text text="" width="auto" id="tQtdNesc" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
                                        </form:fields>
                                    </form:FormElement>
                                    <form:FormElement id="_IDGenFormElement7" visible="true">
                                        <form:label>
                                            <Label id="_IDGenLabel7" text="{i18n>lblQtdEnviada}" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
                                        </form:label>
                                        <form:fields>
                                            <Text text="" width="auto" id="tQtdEnviada" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
                                        </form:fields>
                                    </form:FormElement>
                                    <form:FormElement id="_IDGenFormElement8" visible="true">
                                        <form:label>
                                            <Label id="_IDGenLabel8" text="{i18n>lblEstoque}" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
                                        </form:label>
                                        <form:fields>
                                            <Text text="" width="auto" id="tEstoque" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
                                        </form:fields>
                                    </form:FormElement>
                                    <form:FormElement id="_IDGenFormElement9" visible="true">
                                        <form:label>
                                            <Label id="_IDGenLabel9" text="{i18n>lblQtdPorCartao}" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
                                        </form:label>
                                        <form:fields>
                                            <Text text="" width="auto" id="tQtdPorCartao" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
                                        </form:fields>
                                    </form:FormElement>
                                    <form:FormElement id="_IDGenFormElement19" visible="true">
                                        <form:label>
                                            <Label id="_IDGenLabel19" text="{i18n>lblQtdNConfirmada}" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
                                        </form:label>
                                        <form:fields>
                                            <Text text="" width="auto" id="tQtdNConfirmada" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
                                        </form:fields>
                                    </form:FormElement>
                                </form:formElements>
                                <form:title/>
                            </form:FormContainer>
                        </form:formContainers>
                        <form:layout>
                            <form:ResponsiveGridLayout id="_IDGenResponsiveGridLayout1" adjustLabelSpan="false" columnsL="2" labelSpanL="3" columnsM="1" labelSpanM="4" labelSpanS="12"/>
                        </form:layout>
                    </form:Form>
                    <Table id="idTableItens" inset="false" items="{/KanbanCardSet}" mode="MultiSelect" growing="true">
                        <headerToolbar>
                            <OverflowToolbar id="_IDGenOverflowToolbar2">
                                <Title id="_IDGenTitle1" text="Items" level="H2"/>
                                <ToolbarSpacer id="_IDGenToolbarSpacer2"/>
                                <Button id="_IDGenButton2" text="{i18n>btnInserir}" press="accessFragment"/>
                                <Button id="_IDGenButton3" text="{i18n>btnDelete}" press="onDeleteTableItem"/>
                            </OverflowToolbar>
                        </headerToolbar>
                        <columns>
                            <Column id="_IDGenColumn1" minScreenWidth="Tablet" demandPopin="true" width="5%">
                                <Text id="_IDGenText1" text="{i18n>lblItem}"/>
                            </Column>
                            <Column id="_IDGenColumn2" minScreenWidth="Tablet" demandPopin="true">
                                <Text id="_IDGenText2" text="{i18n>lblCartaoUnico}"/>
                            </Column>
                            <Column id="_IDGenColumn3" minScreenWidth="Tablet" demandPopin="true" >
                                <Text id="_IDGenText3" text="{i18n>lblQtdDeCartao}"/>
                            </Column>
                            <Column id="_IDGenColumn12" minScreenWidth="Tablet" demandPopin="true">
                                <Text id="_IDGenText20" text="{i18n>lblQtdPorCartao}"/>
                            </Column>
                            <Column id="_IDGenColumn4" minScreenWidth="Tablet" demandPopin="true" >
                                <Text id="_IDGenText4" text="{i18n>lblUn}"/>
                            </Column>
                            <Column id="_IDGenColumn5" minScreenWidth="Tablet" demandPopin="true">
                                <Text id="_IDGenText5" text="{i18n>lblUtilizarLivre}"/>
                            </Column>
                            <Column id="_IDGenColumn6" minScreenWidth="Tablet" demandPopin="true">
                                <Text id="_IDGenText6" text="{i18n>lblStatusImp}"/>
                            </Column>
                        </columns>
                        <items>
                            <ColumnListItem id="_IDGenColumnListItem1" vAlign="Middle">
                                <cells>
                                    <Text id="_IDGenText21" text="{Item}"/>
                                    <CheckBox id="_IDGenCheckBox1" selected="{=${Unico}==='' ? false : true}" enabled="false"/>
                                    <Text id="_IDGenText9" text="{QtdCartoes}"/>
                                    <Text id="_IDGenText19" text="{QtdPCartao}"/>
                                    <Text id="_IDGenText10" text="{Um}"/>
                                    <CheckBox id="_IDGenCheckBox2" selected="{=${UtilizarEstoque}==='' ? false : true}" enabled="false"/>
                                    <Text id="_IDGenText11" text="{Status}"/>
                                </cells>
                            </ColumnListItem>
                        </items>
                    </Table>
                </content>
                <footer>
                    <OverflowToolbar id="otbFooter">
                        <ToolbarSpacer id="_IDGenToolbarSpacer3"/>
                        <Button id="Save" text="{i18n>btnSalvar}" press="onSave" visible="true" type="Accept"/>
                        <Button id="CancelView2" text="{i18n>btnCancel}" press="_onPageNavButtonPress" visible="true"/>
                    </OverflowToolbar>
                </footer>
            </Page>
        </pages>
    </App>
</mvc:View>